              schema:
                $ref: '#/components/schemas/Error'
    post:
      summary: Count the number of Contract by ContractFilterOptions
      tags:
        - Contract
      requestBody:
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/ContractFilterOptions'
            examples:
              countWithMultipleCriteria:
                summary: Count with multiple criteria
                value:
                  $filter:
                    notEquals:
                      Id: 33
                    contains:
                      Number: "abcd"
                      Url: "abcd"
                      Comment: "abcd"
                    greaterThan:
                      Id: 0
                    lessThanOrEqual:
                      Id: 100
              countWithEquals:
                summary: Count with Equals
                value:
                  $filter:
                    equals:
                      Id: 0
              countWithNotEquals:
                summary: Count with Not Equals
                value:
                  $filter:
                    notEquals:
                      Id: 0
              countWithContains:
                summary: Count with Contains
                value:
                  $filter:
                    contains:
                      Number: "abcd"
                      Url: "abcd"
                      Comment: "abcd"
              countWithGreaterThan:
